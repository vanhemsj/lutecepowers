<?xml version="1.0" encoding="UTF-8"?><plug-in>
    <name>referencelist</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.1.5-SNAPSHOT</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>referencelist.plugin.description</description>
    <provider>referencelist.plugin.provider</provider>
    <provider-url>https://fr.lutece.paris.fr/</provider-url>
    <icon-url>images/admin/skin/feature_default_icon.png</icon-url>
    <copyright>Mairie de paris</copyright>
    <db-pool-required>1</db-pool-required>

    <core-version-dependency>
        <min-core-version>4.1.0</min-core-version>
        <max-core-version/>
    </core-version-dependency>

    <!-- Admin features parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>REFERENCELIST_MANAGEMENT</feature-id>
            <feature-title>referencelist.adminFeature.ReferenceListManage.name</feature-title>
            <feature-description>referencelist.adminFeature.ReferenceListManage.description</feature-description>
            <feature-level>0</feature-level>
            <feature-url>jsp/admin/plugins/referencelist/ManageReferences.jsp</feature-url>
            <feature-icon-url/>
        </admin-feature>
    </admin-features>

    <!-- XPages parameters -->
    <applications>
    </applications>

    <!-- Portlet parameters -->
    <portlets>
    </portlets>

    <!-- RBAC Resources -->
    <rbac-resource-types>
        <rbac-resource-type>
            <rbac-resource-type-class>
                fr.paris.lutece.plugins.referencelist.service.ReferenceIdService
            </rbac-resource-type-class>
        </rbac-resource-type>
    </rbac-resource-types>
</plug-in>